<h2>Track Your Claims</h2>
<table class="table">
    <thead>
        <tr>
            <th>Lecturer</th>
            <th>Hours Worked</th>
            <th>Hourly Rate</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)

        {
            <tr>
                <td>@claim.LecturerName</td>
                <td>@claim.HoursWorked</td>
                <td>@claim.HourlyRate</td>
                <td>@claim.Status</td>
            </tr>
        }
    </tbody>
</table>
